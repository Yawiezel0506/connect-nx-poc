{
  "name": "@connect-nx/express-example",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "express-example",
    "targets": {
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "express-example:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "express-example:build:development"
          },
          "production": {
            "buildTarget": "express-example:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  }
}
